<body>
<script>
	// Array를 이용한 배열 생성
	
	// 배열 생성
	// 데이터 타입이 달라도 같은 배열에 담을 수 있음
	var arr = new Array();
	
	arr[2] = 100;
	arr[5] = "홍길동";
	
	for(i=0; i<arr.length; i++){
		document.write("arr[" + i + "]" + arr[i]+"<br/>");
		
	}
	
</script>
</body>
